                "I": {
                    "type": "number",
                    "example": 5.5
                },
                "TTL": {
                    "type": "number",
                    "example": 120
                },
                "current_iteration": {
                    "type": "integer",
                    "example": 3
                },
                "current_value": {
                    "type": "number",
                    "example": 21.33
