                "d": {
                    "type": "number",
                    "example": 11.5
                },
                "n": {
                    "type": "integer",
                    "example": 5
                },
                "n1": {
                    "type": "number",
                    "example": 21.33
                },
                "queue_index": {
                    "type": "integer",
                    "example": 0
                },
                "task_created_at": {
                    "type": "string",
                    "example": "2022-01-01T00:00:00+00:00"
                },
                "task_finished_at": {
